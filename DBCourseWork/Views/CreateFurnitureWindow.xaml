<Window x:Class="DBCourseWork.Views.CreateFurnitureWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DBCourseWork.Views"
        mc:Ignorable="d"
        Title="CreateFurnitureWindow" Height="250" Width="200">
    <StackPanel Margin="15">
        <Label Content="Order"></Label>
        <ComboBox Name="cb_Order" />

        <Label Content="Address"></Label>
        <TextBox Name="tb_address" />

        <Button Click="bt_CreateFurniture_Click" Content="Create" Name="bt_CreateFurniture"/>
    </StackPanel>
</Window>

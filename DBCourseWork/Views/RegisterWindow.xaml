<Window x:Class="DBCourseWork.Views.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DBCourseWork.Views"
        mc:Ignorable="d"
        Title="RegisterWindow" Height="250" Width="200">
    <StackPanel Margin="15">
        <Label Content="Login"></Label>
        <TextBox Name="tb_login"></TextBox>

        <Label Content="Password"></Label>
        <TextBox Name="tb_password"></TextBox>

        <Button Name="bt_login" Content="Login" Click="bt_login_Click"></Button>
    </StackPanel>
</Window>
